: none;\n  }\n  &::placeholder {\n    color: grey;\n  }\n`;\n\nconst Button = styled.button`\n  line-height: 1;\n  pointer-events: ${props => (props.barOpened ? \"auto\" : \"none\")};\n  cursor: ${props => (props.barOpened ? \"pointer\" : \"none\")};\n  background-color: transparent;\n  border: none;\n  outline: none;\n  color: black;\n`;\n\nfunction SearchBar() {\n  const [input, setInput] = useState(\"\");\n  const [barOpened, setBarOpened] = useState(false);\n  const formRef = useRef();\n  const inputFocus = useRef();\n  const dispatch = useDispatch();\n\n  const onFormSubmit = e => {\n    e.preventDefault();\n    setInput(\"\");\n    setBarOpened(false);\n    dispatch(getPostcodeCoordinates(input))\n  };\n  \n  return (<>\n      <Form\n        barOpened={barOpened}\n        onClick={() => {\n          setBarOpened(true);\n          if(inputFocus.current) {\n            inputFocus.current.focus();\n          }\n        }}\n        onFocus={() => {\n          setBarOpened(true);\n          if(inputFocus.current) {\n            inputFocus.current.focus();\n          }\n        }}\n        onBlur={() => {\n          setBarOpened(false);\n        }}\n        onSubmit={onFormSubmit}\n        ref={formRef}\n      >\n        {!barOpened && <Button type=\"submit\" barOpened={barOpened}>\n          Click to search\n        </Button>}\n       { barOpened && <Input\n          onChange={e => setInput(e.target.value)}\n          ref={inputFocus}\n          value={input}\n          barOpened={barOpened}\n          placeholder=\"Search by postcode, address or city\"\n        />}\n      </Form>\n      \n    </>\n  );\n}\n\nexport default SearchBar;","/Users/mivd/Documents/Code/assignments/oberon/obrewon/src/components/search/SearchResults.js",[],"/Users/mivd/Documents/Code/assignments/oberon/obrewon/src/components/map/ViewControl.js",[],"/Users/mivd/Documents/Code/assignments/oberon/obrewon/src/components/locations/Brewery.js",["86"],"import { useEffect, useState } from \"react\